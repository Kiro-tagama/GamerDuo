{
  "expo": {
    "name": "GamerDuo",
    "slug": "gamerduo",
    "version": "1.0.0",
    "orientation": "portrait",
    "notification":{
      "icon":"./assets/icon.png"
    },
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#222"
      }
    },
    "web": {
      "favicon": "./assets/min-icon-2x.png"
    },
    "extra": {
      "eas": {
        "projectId": "142717a3-e41a-44d9-bcd1-e7b2069ba4a8"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/142717a3-e41a-44d9-bcd1-e7b2069ba4a8"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
        "android": {
          "compileSdkVersion": 31,
          "targetSdkVersion": 31,
          "buildToolsVersion": "31.0.0"
        },
        "ios": {
          "deploymentTarget": "13.0"
        }}
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png"
        }
      ]
    ]
  }
}
